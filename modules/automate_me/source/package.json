{
    "name": "automate_me",
    "version": "1.0.1",
    "description": "create bash scripts and run them",
    "main": "index.js",
    "scripts": {
        "docs": "rm -rf docs && ./node_modules/.bin/jsdoc --configure ./jsdoc.json -d docs --verbose",
        "play": "node playground.js",
        "deploy": "npm run test && git add . && git commit -m 'updated' && git push && npm version patch && npm publish",
        "test": "node __tests__/index.js",
        "compile": "rimraf dist && babel src -d dist",
        "build": "rimraf dist/ && webpack"
    },
    "keywords": [
        "bash",
        "automate"
    ],
    "author": "zen-out",
    "license": "MIT",
    "dependencies": {
        "child_process": "^1.0.2",
        "clear_concise_creative": "^1.0.6",
        "whats_wrong": "latest"
    },
    "homepage": "https://zen-out.github.io/packages/automate_me",
    "repository": "https://github.com/zen-out/automate_me",
    "devDependencies": {
        "@babel/cli": "latest",
        "@babel/core": "latest",
        "@babel/node": "latest",
        "@babel/plugin-proposal-class-properties": "latest",
        "@babel/polyfill": "latest",
        "@babel/preset-env": "latest",
        "babel-loader": "latest",
        "code_clarity": "latest",
        "copy-webpack-plugin": "latest",
        "explain_me": "^1.3.1",
        "gulp": "latest",
        "gulp-mocha": "latest",
        "node_basetest": "latest",
        "rimraf": "latest",
        "webpack": "latest",
        "webpack-cli": "latest"
    },
    "exports": {
        ".": "./dist/index.js"
    }
}
