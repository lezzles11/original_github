{
    "name": "make_legit",
    "version": "1.1.1",
    "description": "cleans and formats data the way you would like",
    "main": "index.js",
    "scripts": {
        "test": "node __tests__/index.js",
        "play": "node playground.js",
        "deploy": "npm run test && git add . && git commit -m 'updated' && git push && npm version patch && npm publish",
        "docs": "rm -rf docs && ./node_modules/.bin/jsdoc --configure ./jsdoc.json -d docs --verbose",
        "compile": "rimraf dist && babel src -d dist",
        "build": "rimraf dist/ && webpack",
        "update": "ncu --upgrade"
    },
    "keywords": [
        "clean data",
        "data",
        "object",
        "array",
        "format"
    ],
    "author": "zen-out",
    "license": "MIT",
    "dependencies": {
        "check-types": "^11.1.2",
        "clean-deep": "^3.4.0",
        "define_me": "^1.2.7",
        "explain_me": "^1.4.9",
        "gulp": "^4.0.2",
        "gulp-mocha": "^8.0.0",
        "whats_wrong": "^1.3.1",
        "when_you_free": "^1.2.5"
    },
    "devDependencies": {
        "@babel/cli": "latest",
        "@babel/core": "latest",
        "@babel/node": "latest",
        "@babel/plugin-proposal-class-properties": "latest",
        "@babel/polyfill": "latest",
        "@babel/preset-env": "latest",
        "babel-loader": "latest",
        "code_clarity": "^1.0.24",
        "copy-webpack-plugin": "latest",
        "gulp": "latest",
        "gulp-mocha": "latest",
        "node_basetest": "latest",
        "rimraf": "latest",
        "webpack": "latest",
        "webpack-cli": "latest"
    },
    "exports": {
        ".": "./dist/index.js"
    },
    "homepage": "https://zen-out.github.io/packages/make_legit",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zen-out/make_legit.git"
    },
    "bugs": {
        "url": "https://github.com/zen-out/make_legit/issues"
    },
    "directories": {
        "doc": "docs"
    }
}