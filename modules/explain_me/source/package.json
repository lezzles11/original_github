{
    "name": "explain_me",
    "version": "1.3.0",
    "description": "given location of class, will generate readme, and index.html explaining documentation ",
    "main": "index.js",
    "scripts": {
        "docs": "rm -rf docs && ./node_modules/.bin/jsdoc --configure ./jsdoc.json -d docs --verbose",
        "test": "node __tests__/index.js",
        "play": "node playground.js",
        "deploy": "git add . && git commit -m 'updated' && git push && npm version patch && npm publish",
        "update": "ncu --upgrade"
    },
    "keywords": [
        "readme",
        "jsdoc",
        "highlight.js",
        "documentation",
        "docs"
    ],
    "author": "zen-out",
    "license": "MIT",
    "dependencies": {
        "automate_me": "^1.0.3",
        "clean-jsdoc-theme": "^3.3.4",
        "clear_concise_creative": "^1.1.1",
        "code_clarity": "^1.0.24",
        "jsdoc": "^3.6.10",
        "jsdoc-to-markdown": "^7.1.1"
    },
    "homepage": "https://zen-out.github.io/packages/explain_me",
    "directories": {
        "doc": "docs"
    },
    "devDependencies": {
        "mocha": "^9.2.2"
    }
}